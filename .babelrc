{
    "presets": [
        ["env", {"modules": false}],
        "stage-0",
        "stage-1",
        "stage-2",
        "stage-3",
        "react"
    ],
    "env": {
        "test": {
            "presets": [
                ["env"],
                "react"
            ]
        }
    },
    "plugins": [
        ["import", [
            {
                "libraryName": "antd-mobile",
                "style": true
            },
            {
                "libraryName": "antd",
                "style": true
            }
        ]
        ],
        [
            "babel-plugin-styled-components",
            {
                "displayName": true
            }
        ],
        [
            "extensible-destructuring",
            {
                "mode": "optout",
                "impl": "immutable"
            }
        ],
        "react-hot-loader/babel",
        "transform-decorators-legacy"
    ]
}